{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "start:docker": "docker compose up --build",
    "start:docker:seed": "docker compose --profile seed up --build",
    "stop:docker": "docker compose down",
    "db:init": "bash scripts/db-init.sh",
    "db:reset": "bash scripts/db-reset.sh",
    "db:seed": "node src/db/seed.js",
    "db:init:node": "node src/db/init.js",
    "db:reset:node": "node src/db/reset.js",
    "db:setup:render": "node scripts/render-db-setup.js",
    "test": "bash scripts/test.sh",
    "test:run": "vitest run"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
